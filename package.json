{
  "name": "vite2-project",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "eslint \"**/*.{vue,ts,js}\"",
    "typecheck": "vue-tsc --noEmit",
    "prepare": "husky install",
    "pre-commit": "lint-staged"
  },
  "dependencies": {
    "@vueuse/components": "^7.4.1",
    "@vueuse/core": "^7.4.1",
    "vue": "^3.2.25"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.14.0",
    "@antfu/eslint-config-ts": "^0.14.0",
    "@antfu/eslint-config-vue": "^0.14.0",
    "@types/node": "^17.0.4",
    "@vitejs/plugin-vue": "^2.0.0",
    "eslint": "^7",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.4",
    "path": "^0.12.7",
    "postcss": "^8.4.5",
    "postcss-load-config": "^3.1.0",
    "postcss-nested": "^5.0.6",
    "typescript": "^4.4.4",
    "vite": "^2.7.2",
    "vue-tsc": "^0.29.8"
  },
  "lint-staged": {
    "*.(js|ts|vue)": [
      "npm run lint --cache"
    ]
  }
}
